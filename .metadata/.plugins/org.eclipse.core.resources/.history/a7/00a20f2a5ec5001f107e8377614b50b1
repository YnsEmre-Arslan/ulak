package com.ars.user.business.response;

import java.sql.Date;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Builder
public class MessageResponse {
	

    private String id;

    private String text;

    private Boolean sent;
	
    private String time;
    
    private Date date;


}
