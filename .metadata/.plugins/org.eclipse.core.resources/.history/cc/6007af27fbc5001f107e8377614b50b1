package com.ars.user.business.response;

import jakarta.persistence.Column;
import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Builder
public interface NotificationResponse {

	
	Integer id;
    
     Boolean message;
	
     Boolean mail;
	
     Boolean news;
	
     Boolean app;
}
